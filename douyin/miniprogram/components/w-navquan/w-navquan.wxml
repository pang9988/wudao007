<view class='view-page' style="padding-top:0rpx;">
  <image src="./../../images/yt/22.png" style="width:100%;height:100%;" mode="widthFix"></image>
  <view class='page-content'>
    <view class="homeaa">
      <!--  内容-->
      <view class="homeab">
      <!-- 首页由两个for循环左右生成的 -->
        <block>
          <view class="homeleft" wx:for="{{cao}}" wx:key="incex">
            <view class="weizi" catchtap="leftdetails" data-sifttapid="{{item.id}}">
              <image style="width:100%;" class="weizi1" mode="widthFix" src="{{item.img}}"></image>
              <view class="homesite">
                <view style="margin-right:25rpx;">
                  <image class="hometou" src="{{item.imgtx}}"></image>
                </view>
              </view>
            </view>
            <view style="margin:30rpx 0 30rpx 23rpx;">
              <view class="xiam">工作地区:
                <text>{{item.textwz}}</text>
              </view>
              <view class="xiam">级别:
                <text>{{item.textwz1}}</text>
              </view>
              <view class="xiam">综合评分:
                <text>{{item.textz2}}</text>
              </view>
            </view>
          </view>
        </block>
      </view>




      <view class="homeab">
        <block>
          <view class="homeleft" wx:for="{{mmp}}" wx:key="index">
            <view class="weizi" catchtap="rightdetails">
              <image style="width:100%;" class="weizi1" mode="widthFix" src="{{item.img}}"></image>
              <view class="homesite">
                <view style="margin-right:25rpx;">
                  <image class="hometou" src="{{item.imgtx}}"></image>
                </view>
              </view>
            </view>
            <view style="margin:30rpx 0 30rpx 23rpx;">
              <view class="xiam">工作地区:
                <text>{{item.textwz}}</text>
              </view>
              <view class="xiam">级别:
                <text>{{item.textwz1}}</text>
              </view>
              <view class="xiam">综合评分:
                <text>{{item.textz2}}</text>
              </view>
            </view>
          </view>
        </block>
      </view>

    </view>




  </view>
</view>
